          "-//Hibernate/Hibernate Mapping DTD 5.3//EN"    
          "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
	<class name="hibernate_mapping.many_to_one_xml.Address9" table="address_9">
		<id name="addressId">
